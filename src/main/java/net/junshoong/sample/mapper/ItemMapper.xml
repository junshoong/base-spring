    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.junshoong.sample.mapper.ItemMapper">

    <select id="getItem" resultType="net.junshoong.sample.domain.Item" parameterType="java.lang.String">
        select * from item where id=#{value}
